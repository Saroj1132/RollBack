<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
        <table border="1">
            <tr>
                <th>Name</th>
                <td>Email</td>
            </tr>
            
            <%records.forEach(function(p){%>
            <tr>
                <td><%=p.Name%></td>
                <td><%=p.Email%></td>
            </tr>

            <% }) %>
    
        </table>
        
    </body>
</html>